{% extends "masterpage.twig" %}

{% block body %}

    <table border="10">
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Lastname</th>
            <th>Age</th>
        </tr>
    {% set i = 0 %}
    {% for user in users %}
        {% set i = i + 1 %}
        <tr>
            <td>{{ i }}</td>
            <td>{{ user.name }}</td>
            <td>{{ user.lastname }}</td>
            <td>{{ user.age }}</td>
        </tr>


    {% endfor %}
    </table>

{% endblock %}